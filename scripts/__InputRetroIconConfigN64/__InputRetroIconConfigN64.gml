InputIconDefineGamepad(INPUT_GAMEPAD_TYPE_N64, gp_shoulderl,  "L"); //L
InputIconDefineGamepad(INPUT_GAMEPAD_TYPE_N64, gp_shoulderr,  "R"); //R
